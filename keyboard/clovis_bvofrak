// ┣━━━━━━━╋━━━━━┷━┳━━━┷━━━┱─┴─────┴─────┴─────┴─────┴─────┴───┲━┷━━━━┳┷━━━━━╋━━━━━━┳━━━━━━┫
// ┃       ┃       ┃       ┃ ␣         Espace fine insécable ⍽ ┃      ┃Bvo   ┃      ┃ LOCK ┃
// ┃ Ctrl  ┃  Win  ┃  Alt  ┃ ␣ Espace       Espace insécable ⍽ ┃AltGr ┃Frak  ┃Azerty┃ NAV  ┃
// ┗━━━━━━━┻━━━━━━━┻━━━━━━━┹───────────────────────────────────┺━━━━━━┻━━━━━━┻━━━━━━┻━━━━━━┛

// ┌─────┬─────┬─────┬─────┬─────┬─────┬─────┬─────┬─────┬─────┬─────┬─────┬─────┲━━━━━━━━━┓
// │     │     │     │     │     │     │     │     │     │     │     │     │     ┃ ⌫ Retour┃
// │ ESC │ F1  │ F2  │ F3  │ F4  │ F5  │ F6  │ F7  │ F8  │ F9  │ F10 │ F11 │ F12 ┃  arrière┃
// ┢━━━━━┷━┱───┴─┬───┴─┬───┴─┬───┴─┬───┴─┬───┴─┬───┴─┬───┴─┬───┴─┬───┴─┬───┴─┬───┺━┳━━━━━━━┫
// ┃       ┃ J } │ D ] │ L ) │ " ´ │ X   │ W Œ │ B € │ O ÷ │ P × │ É ± │ !   │ K   ┃Entrée ┃
// ┃Tab ↹  ┃ j { │ d [ │ l ( │ ' ` │ x ù │ w œ │ b $ │ o \ │ p % │ é @ │ ?   │ k   ┃   ⏎   ┃
// ┣━━━━━━━┻┱────┴┬────┴┬────┴┬────┴┬────┴┬────┴┬────┴┬────┴┬────┴┬────┴┬────┴┬────┺┓      ┃
// ┃        ┃ N 5 │ T 6 │ R 7 │ S 8 │ M 9 │ ;   │ U _ │ E & │ I # │ A | │ Z   │ Ç   ┃      ┃
// ┃Maj ⇬   ┃ n 0 │ t 1 │ r 2 │ s 3 │ m 4 │ , = │ u / │ e * │ i + │ a - │ z ~ │ ç   ┃      ┃
// ┣━━━━━━━┳┹────┬┴────┬┴────┬┴────┬┴────┬┴────┬┴────┬┴────┬┴────┬┴────┬┴────┲┷━━━━━┻━━━━━━┫
// ┃       ┃ Q > │ F   │ H   │ C   │ V   │ ¨ ´ │ :   │ G   │ È   │ Y   │ À   ┃             ┃
// ┃ Shift ┃ q < │ f   │ h   │ c   │ v ~ │ ^ ` │ .   │ g   │ è   │ y   │ à   ┃    Shift    ┃
// ┣━━━━━━━╋━━━━━┷━┳━━━┷━━━┱─┴─────┴─────┴─────┴─────┴─────┴───┲━┷━━━━┳┷━━━━━╋━━━━━━┳━━━━━━┫
// ┃       ┃       ┃       ┃ ␣         Espace fine insécable ⍽ ┃      ┃Bvo   ┃      ┃ LOCK ┃
// ┃ Ctrl  ┃  Win  ┃  Alt  ┃ ␣ Espace       Espace insécable ⍽ ┃AltGr ┃Frak  ┃Azerty┃ NAV  ┃
// ┗━━━━━━━┻━━━━━━━┻━━━━━━━┹───────────────────────────────────┺━━━━━━┻━━━━━━┻━━━━━━┻━━━━━━┛

// ┌─────┬─────┬─────┬─────┬─────┬─────┬─────┬─────┬─────┬─────┬─────┬─────┬─────┲━━━━━━━━━┓
// │     │     │     │     │     │     │     │     │     │     │     │     │     ┃         ┃
// │ ESC │     │     │     │     │     │     │     │     │     │     │     │     ┃         ┃
// ┢━━━━━┷━┱───┴─┬───┴─┬───┴─┬───┴─┬───┴─┬───┴─┬───┴─┬───┴─┬───┴─┬───┴─┬───┴─┬───┺━┳━━━━━━━┫
// ┃       ┃ACL0 │ACL1 │ACL2 │     │     │     │     │     │     │     │     │     ┃       ┃
// ┃       ┃Mouse│Mouse│Mouse│     │     │     │     │     │     │     │     │     ┃       ┃
// ┣━━━━━━━┻┱────┴┬────┴┬────┴┬────┴┬────┴┬────┴┬────┴┬────┴┬────┴┬────┴┬────┴┬────┺┓      ┃
// ┃        ┃Wheel│Wheel│Wheel│Wheel│     │Mouse│Mouse│Mouse│Mouse│Right│     │     ┃      ┃
// ┃        ┃Left │Down │ Up  │Right│     │Left │Down │ Up  │Right│Click│     │     ┃      ┃
// ┣━━━━━━━┳┹────┬┴────┬┴────┬┴────┬┴────┬┴────┬┴────┬┴────┬┴────┬┴────┬┴────┲┷━━━━━┻━━━━━━┫
// ┃       ┃     │     │     │     │     │     │     │     │     │     │     ┃             ┃
// ┃       ┃     │     │     │     │     │     │     │     │     │     │     ┃             ┃
// ┣━━━━━━━╋━━━━━┷━┳━━━┷━━━┱─┴─────┴─────┴─────┴─────┴─────┴───┲━┷━━━━┳┷━━━━━╋━━━━━━┳━━━━━━┫
// ┃       ┃       ┃       ┃                Left               ┃      ┃      ┃      ┃      ┃
// ┃       ┃       ┃       ┃                Click              ┃      ┃      ┃      ┃      ┃
// ┗━━━━━━━┻━━━━━━━┻━━━━━━━┹───────────────────────────────────┺━━━━━━┻━━━━━━┻━━━━━━┻━━━━━━┛

// ┌─────┬─────┬─────┬─────┬─────┬─────┬─────┬─────┬─────┬─────┬─────┬─────┬─────┲━━━━━━━━━┓
// │     │     │     │     │     │     │     │     │     │     │     │     │     ┃         ┃
// │     │     │     │     │     │     │     │     │     │     │     │     │     ┃         ┃
// ┢━━━━━┷━┱───┴─┬───┴─┬───┴─┬───┴─┬───┴─┬───┴─┬───┴─┬───┴─┬───┴─┬───┴─┬───┴─┬───┺━┳━━━━━━━┫
// ┃       ┃     │     │     │     │     │     │     │     │     │     │     │     ┃       ┃
// ┃       ┃     │     │     │     │     │     │     │     │     │     │     │     ┃       ┃
// ┣━━━━━━━┻┱────┴┬────┴┬────┴┬────┴┬────┴┬────┴┬────┴┬────┴┬────┴┬────┴┬────┴┬────┺┓      ┃
// ┃        ┃     │     │     │     │     │Mouse│     │     │     │     │     │     ┃      ┃
// ┃        ┃     │     │     │     │     │Left │Down │ Up  │Right│     │     │     ┃      ┃
// ┣━━━━━━━┳┹────┬┴────┬┴────┬┴────┬┴────┬┴────┬┴────┬┴────┬┴────┬┴────┬┴────┲┷━━━━━┻━━━━━━┫
